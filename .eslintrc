{
  "extends": ["eslint:recommended"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "function-paren-newline": 0,
    "object-curly-newline": 0,
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "comma-dangle": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-param-reassign": 1,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0
  }
}
